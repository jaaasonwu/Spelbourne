  },
  "homepage": "https://github.com/zhaih/web-tech#readme",
  "dependencies": {
    "base64url": "^2.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.1",
    "concurrently": "^3.4.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "express-session": "^1.15.1",
    "google-auth-library": "^0.10.0",
    "method-override": "^2.3.7",
    "mongoose": "^4.8.7",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
